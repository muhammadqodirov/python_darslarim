ела");if(-1!==t.indexOf("unexpected indent")){var n=window.__letpy.source;return-1!==n.indexOf("if")||-1!==n.indexOf("elif")||-1!==n.indexOf("else")||-1!==n.indexOf("for")||-1!==n.indexOf("while")?"Отступ на строке ".concat(e.lineNumber," неправильный или не нужен совсем. Помните, что отступ в Python — это 4 пробела."):"Отступ на строке ".concat(e.lineNumber," скорее всего не нужен совсем. Помните, что без надобности отступать от левого края в Python нельзя.")}},TypeError:function(e){var t=e.exception.args[0];if(-1!==t.indexOf("unsupported operand type(s) for")){var n=t.replace("unsupported operand type(s) for ","").split(":"),i=n[0];n=n[1].split("'").join("").replace(/ /g,""),n=n.split("and");var s="Вы ошиблись в программе на строке ".concat(e.lineNumber);if(n[0]===n[1]){var o=n[0].replace("int"," с числами").replace("str"," со строками").replace("float"," с числами").replace("list"," со списками");return"".concat(s,'. Нельзя использовать оператор "').concat(i,'" ').concat(o,".")}var a=n[0].replace("int","с числом").replace("str","со строкой").replace("float","с числом").replace("list","со списком"),r=n[1].replace("int","и числом").replace("str","и строкой").replace("float","и числом").replace("list","и списком");return"".concat(s,'. Нельзя использовать оператор "').concat(i,'" ').concat(a," ").concat(r,". Это разные типы данных!")}if(-1!==t.indexOf("unorderable types:")){var c=t.replace("unorderable types:","").replace(/\(\)/g,"").replace(/'/g,"");return c=c.replace(">=","и").replace(">","и").replace("<","и"),c=c.replace(/str/g,"строку").replace(/int/g,"число").replace(/float/g,"число").replace(/list/g,"список").replace(/tuple/g,"кортеж"),"Вы ошиблись на строке ".concat(e.lineNumber,". Нельзя сравнивать ").concat(c,".")}if(-1!==t.indexOf("not supported between instances of")){var l=t.replace("not supported between instances of","").replace(/\(\)/g,"").replace(/'/g,"");return l=l.replace(">=","").replace(">","").replace("<",""),l=l.replace("and","и"),l=l.replace(/s